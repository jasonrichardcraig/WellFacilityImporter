<Application x:Class="EnerSync.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:locators="clr-namespace:EnerSync.Locators"
             xmlns:local="clr-namespace:EnerSync"
             StartupUri="Views/MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary>
                    <locators:ViewModelLocator x:Key="Locator" />
                </ResourceDictionary>
                <ResourceDictionary Source="Resources/ApplicationStyles.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
